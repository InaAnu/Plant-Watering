CREATE VIEW full_event_info AS SELECT events._id AS event_id, plant_information.scientific_name, plant_aliases.alias, plant_information.type AS plant_type, plant_information.watering_recurrence, events.type AS event_type, events.start_date, events.end_date FROM plant_information INNER JOIN plant_aliases ON plant_information._id = plant_aliases.scientific_name INNER JOIN events ON plant_information._id = events.plant ORDER BY events._id, plant_information.scientific_name, plant_aliases.alias, plant_information.type, plant_information.watering_recurrence, events.type, events.start_date, events.end_date</sql><current_tab id="1"/></tab_sql></sqlb_project>
